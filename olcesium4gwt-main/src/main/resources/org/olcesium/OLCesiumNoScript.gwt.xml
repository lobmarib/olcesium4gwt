<module>
    <inherits name="com.google.gwt.user.User" />
    <inherits name="com.google.gwt.json.JSON" />

    <inherits name="org.cesiumjs.CesiumNoScript"/>
    <!--<inherits name="org.openlayers.OpenLayersNoScript"/>-->
    <inherits name="org.olcesium.OLCesiumScript"/>
    <inherits name='ol.GwtOL'/>

    <entry-point class="org.olcesium.olcs.OLCesiumEntryPoint" />

    <define-linker name="olCesiumScriptInjector" class="org.olcesium.linker.OLCesiumScriptInjector" />
    <add-linker    name="olCesiumScriptInjector"/>

    <set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>

    <source path="olcs"/>
